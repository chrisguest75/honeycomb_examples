{
  "name": "01_helloworld_typescript_opentelemetry",
  "version": "1.0.0",
  "description": "Demonstrates a simple cmdline application (copy these steps)",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "clean:build": "npm run rebuild",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "npm run rebuild && nodemon",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "nodemon.json",
      "tsconfig.json",
      "package.json",
      ".env"
    ],
    "ext": "ts",
    "ignore": [],
    "exec": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.5",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/auto-instrumentations-node": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "pino": "^7.5.1"
  }
}
