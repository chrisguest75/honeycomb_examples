{
  "name": "03_simple_express",
  "version": "1.0.0",
  "description": "Demonstrates a simple cmdline application (copy these steps)",
  "main": "./src/index.js",
  "scripts": {
    "tsc": "tsc",
    "audit": "better-npm-audit audit",
    "clean": "rimraf build",
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "clean:build": "npm run rebuild",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "npm run rebuild && nodemon ",
    "start:dev:debug": "npm run rebuild && node --inspect-brk=0.0.0.0:9229 ./build/src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "docker:build": "docker build --target PRODUCTION -f Dockerfile --label \"org.opencontainers.image.created=$(date '+%Y-%m-%dT%H:%M:%SZ')\" --label \"org.opencontainers.image.version=${githubsha}\" --label \"org.opencontainers.image.url=$(git remote get-url origin)\" -t 03_simple_express .",
    "docker:run": "npm run docker:build && docker run --rm -it -p 8000:8000 --name 03_simple_express 03_simple_express",
    "docker:run:debug": "npm run docker:build && docker run -it --rm -p 8000:8000 -p 9229:9229 --name 03_simple_express 03_simple_express --inspect-brk=0.0.0.0:9229 /work/src/index.js",
    "docker:rebuild": "docker build --no-cache --target PRODUCTION -f Dockerfile --label \"org.opencontainers.image.created=$(date '+%Y-%m-%dT%H:%M:%SZ')\" --label \"org.opencontainers.image.version=${githubsha}\" --label \"org.opencontainers.image.url=$(git remote get-url origin)\" -t 03_simple_express .",
    "docker:stop": "docker stop 03_simple_express",
    "docker:compose:start": "docker compose --env-file ./.env -f ./docker-compose.yaml up --build --force-recreate"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "nodemon.json",
      "tsconfig.json",
      "package.json"
    ],
    "ext": "ts",
    "ignore": [],
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-express": "^1.12.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.200.0",
    "@grpc/grpc-js": "^1.5.7",
    "@opentelemetry/api": "^1.2.0",
    "@opentelemetry/auto-instrumentations-node": "^0.33.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.33.0",
    "@opentelemetry/instrumentation-http": "^0.33.0",
    "@opentelemetry/sdk-node": "^0.33.0",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-pino-logger": "^7.0.0",
    "md5-file": "^5.0.0",
    "pino": "^7.8.1",
    "pubsub-js": "^1.9.4",
    "s3-sync-client": "^3.0.3"
  }
}
